<h2>Products</h2>

{{#if product}}
  <div class="product">
    <h3>Id: {{product.id}}</h3>
    <h3>Name: {{product.name}}</h3>
    <h3>Price: {{product.price}}</h3>
    <h3>Inventory: {{product.inventory}}</h3>
  </div>
{{/if}}

{{#each products}}
  <div class="product">
    <h3>Id: {{id}}</h3>
    <h3>Name: {{name}}</h3>
    <h3>Price: {{price}}</h3>
    <h3>Inventory: {{inventory}}</h3>
  </div>
{{/each}}